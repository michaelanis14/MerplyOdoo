<odoo>
  <data>

              <record id="airWaybill_form" model="ir.ui.view">
                  <field name="name">Air Waybill Form</field>
                  <field name="model">project.main</field>

                  <field name="arch" type="xml">

                          <form>

                              <group>
                                  <group>
                                      <field name="shipper"/>
                                      <field name="consignee"/>

                                  </group>

                                   <group>

                                       <field name="agent_name"/>
                                       <field name="AITA_code"/>
                                       <field name="account_no"/>
                                  </group>

                              </group>

                              <notebook>
                                  <page string="Routing and distnation">
                                      <group>
                                          <group>
                                              <field name="airport_of_departure"/>
                                              <field name="airportsO2M_connector">
                                                  <tree string="Airports" editable='True'>
                                                      <field name="to"/>
                                                      <field name="by"/>
                                                  </tree>
                                              </field>

                                          </group>

                                          <group>
                                              <field name="airport_of_destination"/>
                                              <field name="flight_no"/>
                                              <field name="flight_date"/>
                                          </group>
                                      </group>
                                  </page>

                                    <page string="Charges Decleration">
                                           <group>
                                                <group>
                                                    <field name="currency_id"/>
                                                    <field name="charge_code"/>
                                                    <field name="declared_carriage"/>
                                                    <field name="declared_customs"/>
                                                    <field name="accouunt_insurance"/>
                                                </group>
                                                <group>
                                                    <field name="wt" widget="radio" string='WT/VAL'/>
                                                    <field name="others" widget="radio"  string='Other'/>
                                                </group>
                                            </group>
                                  </page>

                                  <page string='Rate Description Item'>
                                      <field name="cargosO2M_connector">
                                          <tree string="Rate Description Item" editable="True">
                                              <field name="no_of_pieces"/>
                                              <field name="gross_weight"/>
                                              <field name="class_rate"/>
                                              <field name="commodity_item_no"/>
                                              <field name="Chargeable_Wight"/>
                                              <field name="rate_charge"/>
                                              <field name="total"/>
                                              <field name="quantity"/>
                                          </tree>
                                      </field>
                                  </page>
                              </notebook>

                              <group>
                                  <group>
                                      <field name="handling_information"/>

                                  </group>
                                  <group>
                                      <field name="sci"/>
                                      <field name="_destination"/>
                                  </group>
                              </group>
                              <notebook>

                                   <page string="Charges Sammary">
                                       <group>
                                           <group>
                                              <field name="weight_charge"/>
                                              <field name="valuation_charge"/>
                                              <field name="tax"/>

                                           </group>
                                           <group>
                                               <field name="Charges_due_agent"/>
                                              <field name="Charges_due_carrier"/>
                                           </group>
                                       </group>
                                  </page>

                                  <page string="charges in Destination currency">
                                      <group>
                                          <group>
                                              <field name="currency_rate"/>
                                              <field name="cc_charges_in_dest_currency"/>
                                              <field name="charges_at_dest"/>

                                          </group>
                                          <group>
                                              <field name="other_charges"/>
                                              <field name="total_collect_charges"/>
                                          </group>
                                      </group>

                                  </page>

                              </notebook>

                          </form>
                  </field>
              </record>


            <record id="project_main_menu_action" model="ir.actions.act_window">
                    <field name="name"> Project </field>
                    <field name="type">ir.actions.act_window</field>
                    <field name="res_model">project.main</field>
                    <field name="view_mode">form,tree,kanban</field>
                    <!--field name="view_type">form</field-->
                    <field name="help" type="html">

                        <p class="oe_view_nocontent_create">

                            Click to add the bill details
                        </p>
                    </field>
                </record>


            <menuitem
                    id="project_main_menu"
                    name="Project"
                    sequence="10"
                    action="project_main_menu_action"/>

  </data>
</odoo>