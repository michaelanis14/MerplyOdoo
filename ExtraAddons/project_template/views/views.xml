<odoo>
  <data>


      <record id="airWaybill_form" model="ir.ui.view">
          <field name="name">Air Waybill Form</field>
          <field name="model">project.main</field>

          <field name="arch" type="xml">

                  <form>

                      <group>
                          <group>
                              <field name="shipper"/>
                              <field name="consignee"/>

                          </group>

                           <group>

                               <field name="agent_name"/>
                               <field name="AITA_code"/>
                               <field name="account_no"/>
                          </group>

                      </group>

                      <notebook>
                          <page string="Routing and distnation">
                                <field name="routingO2M_connector" widget="one2many_list">
                                    <tree string="Routing and distnation" editable="bottom">
                                      <field name="airport_of_departure"/>
                                      <field name="to"/>
                                      <field name="by"/>
                                      <field name="airport_of_destination"/>
                                      <field name="flight_no"/>
                                      <field name="flight_date"/>
                                    </tree>
                                </field>
                          </page>

                            <page string="Charges Decleration">
                                <field name="chargesO2M_connector">
                                   <group>
                                        <group>
                                            <field name="currency"/>
                                            <field name="charge_code"/>
                                            <field name="declared_carriage"/>
                                            <field name="declared_customs"/>
                                            <field name="declared_customs"/>
                                        </group>
                                        <group>
                                            <group>
                                                <p>WT/VAL</p>
                                                <field name="ppd"/>
                                                <field name="coll"/>
                                            </group>
                                            <group>
                                                <p>Other</p>
                                                <field name="ppd"/>
                                                <field name="coll"/>
                                            </group>
                                        </group>
                                    </group>
                                </field>
                          </page>
                      </notebook>

                      <group>
                          <group>
                              <field name="handling_information"/>

                          </group>
                          <group>
                              <field name="sci"/>
                              <field name="_destination"/>
                          </group>
                      </group>

                       <group>
                          <group>
                              <field name="no_of_pieces"/>
                              <field name="gross_weight"/>
                              <field name="Chargeable_Wight"/>
                              <field name="commodity_item_no"/>
                              <field name="rate_charge"/>
                              <field name="tax"/>
                          </group>
                          <group>

                              <field name="quantity"/>
                          </group>
                      </group>




                  </form>
            </field>
        </record>

    <record id="project_main_menu_action" model="ir.actions.act_window">
            <field name="name"> Project </field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">project.main</field>
            <field name="view_mode">form,tree,kanban</field>
            <!--field name="view_type">form</field-->
            <field name="help" type="html">

                <p class="oe_view_nocontent_create">

                    Click to add the bill details
                </p>
            </field>
        </record>


    <menuitem
            id="project_main_menu"
            name="Project"
            sequence="10"
            action="project_main_menu_action"/>



